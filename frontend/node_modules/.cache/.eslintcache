os from 'axios';\r\nimport { useAppContext } from '../../context/AppContext';\r\nexport const Header = () => {\r\n  const navigate = useNavigate();\r\n  const [cartCount, setCartCount] = useState(0);\r\n  const { sellerId } = useParams();\r\n\r\n  const { user, setUser } = useAppContext();\r\n\r\n  // useEffect(() => {\r\n  //   if (user?.id) {\r\n  //     axios.get(`http://localhost:3003/api/cart/user/${user.id}`)\r\n  //       .then((res) => {\r\n  //         const total = res.data.reduce((acc, item) => acc + item.quantity, 0);\r\n  //         setCartCount(total);\r\n  //       })\r\n  //       .catch((err) => {\r\n  //         console.error('Lỗi lấy giỏ hàng:', err);\r\n  //       });\r\n  //   } else {\r\n  //     setCartCount(0);\r\n  //   }\r\n  // }, [user]);\r\n  useEffect(() => {\r\n  if (user?.id) {\r\n    axios.get(`http://localhost:3003/api/cart/user/${user.id}`)\r\n      .then((res) => {\r\n        const items = res.data.items || [];\r\n        const total = items.reduce((acc, item) => acc + item.quantity, 0);\r\n        setCartCount(total);\r\n      })\r\n      .catch((err) => {\r\n        console.error('Lỗi lấy giỏ hàng:', err);\r\n        setCartCount(0);\r\n      });\r\n  } else {\r\n    setCartCount(0);\r\n  }\r\n}, [user]);\r\n\r\n\r\n  const handleLogout = () => {\r\n    localStorage.removeItem('token');\r\n    localStorage.removeItem('user');\r\n    setUser(null);\r\n    navigate('/login');\r\n  };\r\n\r\n  return (\r\n    <header className='header'>\r\n      <Row className='header__top' justify=\"space-between\">\r\n        <Col>\r\n          <ul className='header__top-left d-flex align-items-center'>\r\n            <li><Link to=\"/home\">Trang chủ Shopee</Link></li>\r\n            <li><Link to=\"/seller/register\">Trở thành Người bán</Link></li>\r\n            <li><Link to=\"/download\">Tải ứng dụng</Link></li>\r\n            <li>\r\n              <span>Kết nối</span>\r\n              <a href=\"#\"><i className=\"fa-brands fa-facebook\"></i></